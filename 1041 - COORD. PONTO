#include<stdio.h>
int main(){

    double p1, p2, quadro;

    scanf("%lf %lf", &p1, &p2);

    if(p1 == 0.0 && p2 == 0.0)
    {
        printf("Origem");
    }
    if(p1 == 0.0 && p2 > 0.0 || p1 == 0.0 && p2 < 0.0)
    {
        printf("Eixo Y");
    }
    if(p1 < 0.0 && p2 == 0.0 || p1 > 0.0 && p2 == 0.0)
    {
        printf("Eixo X");
    }
    if(p1 > 0.0 && p2 > 0.0)
    {
        printf("Q1");
    }
    if(p1 > 0.0 && p2 < 0.0)
    {
        printf("Q4");
    }
    if(p1 < 0.0 && p2 <0.0)
    {
        printf("Q3");
    }
    if(p1 < 0.0 && p2 > 0.0)
    {
        printf("Q2");

    }

    return 0;
}
